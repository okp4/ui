    "react-redux": "^8.0.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "short-uuid": "^4.2.0",
    "ts-bus": "^2.3.1"
  },
  "devDependencies": {
